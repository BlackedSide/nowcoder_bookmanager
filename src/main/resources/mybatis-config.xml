<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="defaultStatementTimeout" value="3000"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="useGeneratedKeys" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="User" type="com.fannyuan.bookmanager.model.User"/>
        <typeAlias alias="Book" type="com.fannyuan.bookmanager.model.Book"/>
        <typeAlias alias="Ticket" type="com.fannyuan.bookmanager.model.Ticket"/>
        <typeAlias alias="Admin" type="com.fannyuan.bookmanager.model.Admin"/>
        <typeAlias alias="UserOrder" type="com.fannyuan.bookmanager.model.UserOrder"/>
    </typeAliases>
</configuration>